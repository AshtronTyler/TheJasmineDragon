    function buildFloatingCart(data) {
    console.log(data);
        var minicart_count = 0;
        var show_prods = '';
        var content_ids = [];
        $.each(data.cart, function(index, prod){
            content_ids.push(prod.sku);
            show_prods += "<div class='minicart_product'>";
            show_prods += "  <img src='" + prod.web_image + " ' width='65' border='0' align='left' />";
            show_prods += "  <div style='margin-left: 65px; margin-bottom: 5px; padding-left:10px' class='overlay_info'>";
            show_prods += "    <div class='prod-info'>" + prod.name + " " + prod.att_list + "</div>";
            show_prods += "    <div class='small'>$" + prod.price + "</div>";
            show_prods += "    <div class='small'>Qty: " + prod.qty + "</div>";
            show_prods += "    <div class='small sku'>Item No. " + prod.sku + "</div>";
            show_prods += "  </div>";
            show_prods += "</div>";
        });

        // Facebook tracking event
                if (typeof fbq != "undefined") {
            fbq('track', 'AddToCart', {
                content_name: 'View Cart',
                content_ids: content_ids,
                content_type: 'product',
                value: data.totals.total_price,
                currency: 'USD'
            });
        }

        $('#minicart_products').html(show_prods);
        $('.minicart_total_price').html(data.totals.total_price);
        $('.minicart_total_qty').html(data.totals.total_qty);
        $('#minicart_overlay_box').slideDown();

        // Pressing Esc (keycode 27) closes the modal, which in turn triggers a page reload.
        
            document.addEventListener('keyup', function(event) {
                var key = event.which || event.keyCode;
                if (key == 27) {
                    $(".minicart_exit").click();  // If the modal should just be closed, use $('#minicart_overlay_box').slideUp();
                }
            });
        

    }

    function optionCheck2(theForm) {
         return true;         for (i = 0; i < theForm.elements.length; i++) {
             if( ( theForm.elements[i].type == "select-one" ) && ( theForm.elements[i].selectedIndex == 0 ) && (theForm.elements[i].name != 'custom_field1') && (theForm.elements[i].name != 'recipient_mapping') && (theForm.elements[i].name.substr(0,18) != 'recurring_interval') ) {

